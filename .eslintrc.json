{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "sort-keys": [
      "error",
      "asc",
      {
        "natural": true
      }
    ],
    "sort-vars": "error"
  },
  "settings": {
    "react": {
      "version": "17"
    }
  }
}
